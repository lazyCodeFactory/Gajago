function goMiniShop(mber_seq) {
	location.href = "/mobile/myMini/selectMiniShop.do?&SELLER_MBER_SEQ=" + mber_seq;
}

function initSwiper(idx){
	if((last_idx - temp_idx > 1 || (temp_idx -idx) > 1) && idx < temp_idx){
		swiper1.slidePrev(null,500);
	}else if((temp_idx > 1 || (idx - temp_idx) > 1) && idx > temp_idx){
		swiper1.slideNext(null,500);
	}
	//console.log(last_idx+"   "+temp_idx+"   "+idx+"   "+swiper1.isEnd);
	temp_idx = idx;
}

function changePlan(idx){
	var planCount = parseInt('${fn:length(result.planList)}');
	for(var i=0 ; i<planCount ; i++){
		if( idx == i){
			$("#PLAN_" + i).show();
		}
		else{
			$("#PLAN_" + i).hide();
		}
	}
	$.cookie('mainSwiperIdx',idx, {expires:1});
	initSwiper(idx);
	mainCdnReload(idx);
	if (idx !=0) {
		$("img.lazyloaded").lazyload();
	}
}

var Request = function()
{
    this.getParameter = function( name )
    {
        var rtnval = "";
        var nowAddress = unescape(location.href);
        var parameters = (nowAddress.slice(nowAddress.indexOf("?")+1,nowAddress.length)).split("&");
 
        for(var i = 0 ; i < parameters.length ; i++)
        {
            var varName = parameters[i].split("=")[0];
            if(varName.toUpperCase() == name.toUpperCase())
            {
                rtnval = parameters[i].split("=")[1];
                break;
            }
        }
        return rtnval;
    }
}
