<%@ page language="java" contentType="text/html; charset=utf-8" pageEncoding="utf-8"%>
<!-- 태그는 cmmnHead.jsp 에 선언함 -->
<%@ include file="/WEB-INF/views/seller/common/cmmnHead.jsp"%>
<script type="text/javascript">
	$(document).ready(function(){
		
	});
</script>
<div class="pop_wrap" style="overflow-y: scroll;">
	<div class="tbl">
		<p class="tbl_tit"><img src="http://static.ssocio.net/admin/images/ico_admin.gif" alt="" />배송조회</p>  
 		<table class="upd">  
			<colgroup>  
				<col style="width:33.4%" />  
				<col style="width:*" />  
			</colgroup>  
 			<tbody>  
				<tr>  
					<th>택배사</th>  
					<td><custom:codeTag code="${list.dlvInfo.DLV_CMP_CD}" groupCode="DLV_CMP_CD" /></td>  
				</tr>  
				<tr>  
					<th>운송장번호</th>  
					<td>${list.dlvInfo.INVC_NO}</td>  
				</tr>
			</tbody>  
		</table>  
  
		<!-- list -->  
		<table class="list">  
			<colgroup>  
				<col style="width:*%" />  
				<col style="width:33.3%" />  
				<col style="width:33.3%" />  
			</colgroup>
			<tbody>
				<c:if test="${empty list.dlvList}">
					<td colspan="3">조회 결과가 없습니다.</td>
				</c:if>
				<c:if test="${not empty list.dlvList}">
					<c:forEach items="${list.dlvList}" var="dlvList" varStatus="dlvStatus">
						<tr>  
							<td>${dlvList.TREAT_DT}</td>  
							<td>${dlvList.TREATREGIPONM}</td>  
							<td>${dlvList.TREATCDNM}</td>  
						</tr>
					</c:forEach>
				</c:if>
			</tbody>  
		</table>

	</div>
	<!-- //tbl -->

	<!-- bot_btn -->
	<div class="bot_btn tc">
		<a href="javascript:self.close();" class="btn_r ml10">확인</a>
	</div>
	<!-- //bot_btn -->
</div>