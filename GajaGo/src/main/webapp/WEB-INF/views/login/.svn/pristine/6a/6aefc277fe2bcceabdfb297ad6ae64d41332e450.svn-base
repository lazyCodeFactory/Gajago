<%@ page language="java" contentType="text/html; charset=utf-8" pageEncoding="utf-8"%>
<script type="text/javascript" src="/js/admin/login.js"></script>
<!-- wrap -->
<div id="wrap">
	<!-- contents -->
	<div id="contents">
		<div class="inner">
			<form id="frmLogon" method="post" name="frmLogon">
				<fieldset>
					<legend>다날 쏘시오 Admin 로그인</legend>
					<div class="login_sect">
						<div class="login_bx">
							<div class="login_info clear">
								<p class="ci"><img src="http://static.ssocio.net/admin/images/logo.png" width="76" height="26" alt="SSOCIO" /></p>
								<p class="txt_info"><span>관리자 아이디로 로그인 해주세요.</span></p>
							</div>
							<div class="login_form">
								<div class="inp_area">
									<span class="inp_txt"><label for="id"></label><input type="text" id="ADM_ID" name="ADM_ID" value="" placeholder="아이디" maxlength="15" style="width:504px;" /></span>
									<span class="inp_txt mt10"><label for="password"></label><input type="password" id="ADM_PSSWD" name="ADM_PSSWD" value="" placeholder="비밀번호" maxlength="15" style="width:504px;" onkeyup="enterReturnKey(event);" /></span>
								</div>
								<a href="javascript:OnLogon();" class="btn_login">로그인</a>
								<div class="save_id mt10">
									<input type="checkbox" id="saveId" name="saveId" value="Y"/><label for="save_id">아이디 저장</label>
								</div>
							</div>
						</div>
					</div>
				</fieldset>
			</form>
		</div>
	</div>
	<!-- //contents -->
</div>
<!-- //wrap -->



