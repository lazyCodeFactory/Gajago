<%@ include file="/WEB-INF/views/web/common/cmmnHead.jsp"%>
<script type="text/javascript">
$(document).ready(function(){
	//공통 HEADER SETTING
	header.type = 0;
// 	if('${params.MBER_SEQ }' != ''){
// 		header.title = "회원전환";
// 	} else {
// 		header.title = "회원가입";
// 	}
	if('${params.JOIN_STATE }' != '0'){
		header.title = "회원전환";
	} else {
		header.title = "회원가입";
	}
	header.backUrl = "/mobile/main/login.do";
	header.method = "GET";
	initHeader();
	//공통 FOOTER SETTING
	displayFooter(false);
	//APP TITLE
// 	if('${params.MBER_SEQ }' != ''){
// 		document.title = "회원전환";
// 	} else {
// 		document.title = "회원가입";
// 	}
	if('${params.JOIN_STATE }' != '0'){
		document.title = "회원전환";
	} else {
		document.title = "회원가입";
	}
});

function goLogin(){

	var CALL_BACK = $("#CALL_BACK").val();
	var url = "";
	if(CALL_BACK == "1"){

		url = "/mobile/myPage/getMyPageInfo.do";
		frm.action = url;
		frm.submit();
	}else if(CALL_BACK == "0"){

		url = "/mobile/main/main.do";
		frm.action = url;
		frm.submit();
	}else{
		if( '${sessionScope.APP}' == 'SSOCIO'){
			if("" != '${sessionScope.user}'){
				console.log("app 접근 and 로그인 된 상태");
				document.location.replace('ssocio://web/autologin?serial_key='+'${sessionScope.user.SERIAL_KEY}');
// 				document.location.replace('ssocio://web/closeWebView');
			}else{
				console.log("app 접근 and 비로그인 된 상태");
				url = "/mobile/main/login.do";
				frm.action = url;
				frm.submit();
			}
		}else{
			url = "/mobile/main/login.do";
			frm.action = url;
			frm.submit();
		}

	}

//	var url = "/mobile/main/login.do";

}

</script>
