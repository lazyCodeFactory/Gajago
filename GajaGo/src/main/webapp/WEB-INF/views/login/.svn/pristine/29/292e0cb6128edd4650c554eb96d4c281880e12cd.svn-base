function numChek(obj){
	var pattern = /^[0-9+]*$/;
	if(!pattern.test($(obj).val())){
		$(obj).val($(obj).val().replace(/[^0-9]/ig, ""));
	}
}

function go_prodDtlPage(prod_code){
