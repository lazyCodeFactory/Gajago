function valiDdlv(){
	if(!$("#CHG_DLV_NAME").val().length > 0){
		alert("배송지 정보를 입력해 주세요.");
		$("#CHG_DLV_NAME").focus();
		return;
	}
	if(!$("#CHG_DLV_PHONE").val().length > 0){
		alert("배송지 정보를 입력해 주세요.");
		$("#CHG_DLV_PHONE").focus();
		return;
	}
	if(!$("#CHG_DLV_ZIP_CODE").val().length > 0){
		alert("배송지 정보를 입력해 주세요.");
		return;
	}
	if(!$("#CHG_DLV_ADDR_DTL").val().length > 0){
		alert("배송지 정보를 입력해 주세요.");
		$("#CHG_DLV_ADDR_DTL").focus();
		return;
	}
	return true;
}


