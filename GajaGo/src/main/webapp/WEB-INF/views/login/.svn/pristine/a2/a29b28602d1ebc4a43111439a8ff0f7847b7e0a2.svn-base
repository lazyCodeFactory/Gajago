public class AdminSession implements Serializable {
	private static final long serialVersionUID = 1L;
	
	private String ADM_ID = "";			//어드민 아이디
	private String ADM_PSSWD = "";			//어드민 아이디
	private String ADM_NM = "";			//관리자명
	private String AUTH_CD = "";			//권한코드
	private String SELLER_CD = "";			//입점사코드
	private String VDR_ID = "";			//밴더ID
	private String HP = "";				//휴대폰번호
	private String EMAIL = "";				//이메일
	private int FAIL_CNT = 0;				//로그인실패횟수
	private boolean isLogin = false;//로그인 여부
	
	public String getADM_ID () {return ADM_ID;}
	public void setADM_ID (String aDM_ID) {ADM_ID = aDM_ID;}
	
	public String getADM_PSSWD () {return ADM_PSSWD;}
	public void setADM_PSSWD (String admPwd) {ADM_PSSWD = admPwd;}
	
	public String getADM_NM () {return ADM_NM;}
	public void setADM_NM (String aDM_NM) {ADM_NM = aDM_NM;}
	
	public String getAUTH_CD () {return AUTH_CD;}
	public void setAUTH_CD (String aUTH_CD) {AUTH_CD = aUTH_CD;}
	
	public String getSELLER_CD () {return SELLER_CD;}  
	public void setSELLER_CD (String sELLER_CD) {SELLER_CD = sELLER_CD;}

	public String getVDR_ID () {return VDR_ID;}  
	public void setVDR_ID (String vDR_ID) {VDR_ID = vDR_ID;}
	
	public String getHP () {return HP;}
	public void setHP (String hP) {HP = hP;}
	
	public String getEMAIL () {return EMAIL;}
	public void setEMAIL (String eMAIL) {EMAIL = eMAIL;}
	
	public int getFAIL_CNT () {return FAIL_CNT;}
	public void setFAIL_CNT (int failCnt) {FAIL_CNT = failCnt;}
	
	public boolean isLogin () {return isLogin;}
	public void setLogin (boolean isLogin) {this.isLogin = isLogin;}
}
