		linkFreeSharePageForm.S_END_DATE.value = linkFreeSharePageForm.S_END_DATE.value.replace(/-/g,'');
		linkFreeSharePageForm.action = "/admin/order/selectListFreeShare.do";
		linkFreeSharePageForm.submit();
	}else if(type == 'RE'){
		linkRentalPageForm.S_START_DATE.value = linkRentalPageForm.S_START_DATE.value.replace(/-/g,'');
		linkRentalPageForm.S_END_DATE.value = linkRentalPageForm.S_END_DATE.value.replace(/-/g,'');
