// 세자리 콤마 찍기
function numFormat(num) {
	var pattern = /(-?[0-9]+)([0-9]{3})/;
	while(pattern.test(num)) {
		num = num.replace(pattern,"$1,$2");
	}
	return num;
}



function imgSubmit(){

	var options = {
		type : "POST",
        url : "/admin/mber/updateCmpnyImg.do",
        success: function(data){
        	//imgValue=COMP_REG_IMG

          var imgValue = $("#imgValue").val();

		  var fileTitle = $('#inp_file')[0].files[0].name;
		  var fileSize = $('#inp_file')[0].files[0].size
		  // 버튼 변경
		  var aHref = "javascript:imgUpdatePop('"+imgValue+"','" + fileTitle + "','" +fileSize+ "');";
		  $("#"+imgValue).attr("href", aHref);

		  // 다운로드 필드 변경
		  var onClick = "javascript:imgDownLoad('" + data.result.fileSeq + "', '" + fileTitle + "');return false;";
		  if($("#"+imgValue).attr("id") == "COMP_REG_IMG"){
			  $("#btnCmpImg").attr("onClick", onClick);
		  }else if($("#"+imgValue).attr("id") == "REG_IMG"){
			  $("#btnRegImg").attr("onClick", onClick);
		  }else if($("#"+imgValue).attr("id") == "BNK_IMG"){
			  $("#btnBnkImg").attr("onClick", onClick);
		  }

		  alert("저장되었습니다.");
		  $(".attach_file_bx").removeClass("on");
		  console.log(data);
        }, error : function(request, status, error) {
		    	alert("통신 오류가 발생 하였습니다. 잠시 후 다시 시도해 주세요"   + status  + " request [" + request  + "] error[" + error +"]");

		 }
	};
	$("#cmpnyImgForm").ajaxSubmit(options);
}

