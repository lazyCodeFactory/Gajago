function imgDownLoad(filePath, title){

	if(filePath==''){
		alert("이미지가 존재 하지 않습니다. ");
		return false;
	}else{

		 $.ajax({
           type: 'post',
           async: true,
           url: '/admin/common/shareImg.do',
           dataType : 'json',//fileseq - > 파일 SEQ, type_cd - > 상품 타입, prod_code - > 상품 코드
           data: {"fileSeq" : filePath,"type_cd" : "4","title" : title},
           success: function(data) {
                 var temp = "";
                 try {

                	 if(data.result.localFilePath == undefined){ // 왜 오류로 안빠지지 ?
                		alert("파일을 다운 받는 도중에 오류가 생겼습니다. 다시 시도해주세요.");
                	 }else{
	               		$('#localFilePath').val(data.result.localFilePath);
	               		$("#fileDownFrm").attr('action', '/admin/common/fileDown.do');
	               		$("#fileDownFrm").submit();
                	 }
                 } catch (e) {
                            console.log(data);
                 }
            },
                 error: function() {
                            console.log('err');
                 }
       });
	}
}

