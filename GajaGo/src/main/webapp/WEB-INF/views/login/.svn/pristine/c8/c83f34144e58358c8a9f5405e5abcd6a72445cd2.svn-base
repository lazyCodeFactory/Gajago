function delImgView(path,img){
	// imgView : 대표 이미지
	// imgView2 : 배경이미지
	if("imgView" == img){
		if("" != $("#MINI_PROFILE_SEQ").val()){
			$("#profChk").val("Del");
		}
	}
	if("imgView2" == img){
		if("" != $("#BG_FILE_SEQ").val()){
			$("#bgChk").val("Del");
		}
	}

	if(path != '' ){
		$("#"+path).val('');
	}

	if(img != '' ){
		$("#"+img).attr("src", baseImg);

	}

	return;
}

