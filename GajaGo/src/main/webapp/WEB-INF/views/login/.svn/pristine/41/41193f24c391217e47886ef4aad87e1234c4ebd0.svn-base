public class CmmnCodeDescriptor {

	// --------------------------------------------------------------------------
	// # Log 설정
	// --------------------------------------------------------------------------
	@SuppressWarnings ("unused")
	private static final Logger logger = LoggerFactory.getLogger(CmmnCodeDescriptor.class);

	// --------------------------------------------------------------------------
	// # 객체/변수 선언
	// --------------------------------------------------------------------------
	private String code    = "";
	private String codeNm = "";

	public CmmnCodeDescriptor(String code, String codeNm) {
		this.code = code;
		this.codeNm  = codeNm;
		//logger.debug(">>>>>[code][codeNm] : [" + code + "][" + codeNm + "]");
	}

	public String getCode() {
		return code;
	}

	public String getCodeNm() {
		return codeNm;
	}
}
