<%@ page language="java" contentType="text/html; charset=utf-8"
    pageEncoding="utf-8"%>
<!-- 태그는 cmmnHead.jsp 에 선언함 -->
<%@ include file="/WEB-INF/views/admin/common/cmmnHead.jsp"%>
<script type="text/javascript">

	$(document).ready(function(){

	});

	// 상품상세 페이지로 이동
	function goProdDtl(prodCode) {

		$("#form").attr("action","/admin/prodMgr/prodReg.do").attr("target", "").submit();

	}

	// 주문메모 저장
	function updateAdminMemo() {

		var url = "/admin/order/updateAdminMemo.do";
		$.ajax({
		    type: 'post'
		    , async: true
		    , dataType:'json'
		    , url: url
		    , data: $("#form").serialize()
		    , success: function(data) {
		    	var rtnMsg = data.rtnMsg;
		    	alert(rtnMsg);
		    }
		    , error: function(data, status, err) {
		    	alert('서버와의 통신이 실패했습니다. 다시 시도해 주십시오.');
		    }
		});

	}

	// 배송지정보 저장
	function updateDlvInfo () {

		if (!confirm("배송지 정보를 저장하시겠습니까?")) {
			return;
		}

		var url = "/admin/order/updateDlvInfo.do";
		$.ajax({
		    type: 'post'
		    , async: true
		    , dataType:'json'
		    , url: url
		    , data: $("#form").serialize()
		    , success: function(data) {
		    	var rtnMsg = data.rtnMsg;
		    	alert(rtnMsg);
		    }
		    , error: function(data, status, err) {
		    	alert('서버와의 통신이 실패했습니다. 다시 시도해 주십시오.');
		    }
		});

	}

	// 배송신청
	function updateFreeShareDlvReq() {

		if (!confirm("배송신청 하시겠습니까?")) {
			return;
		}
		$("#procStatCd").val("22");
		$("#ordStatCd").val("40");

		var url = "/admin/order/updateFreeShareDlvReq.do";
		$.ajax({
		    type: 'post'
		    , async: true
		    , dataType:'json'
		    , url: url
		    , data: $("#form").serialize()
		    , success: function(data) {
		    	var rtnMsg = data.rtnMsg;
		    	alert(rtnMsg);

		    	$("#form").attr("action","/admin/order/selectFreeShareConfDtl.do").attr("target", "").submit();

		    }
		    , error: function(data, status, err) {
		    	alert('서버와의 통신이 실패했습니다. 다시 시도해 주십시오.');
		    }
		});

	}

	// 상품배송
	function procDlvProd() {

		var dlvWayCd = $("#dlvWayCd").val();
		if (dlvWayCd == "2") {
			procInvcInfoPop("show");
		}
		else {
			updateFreeShareProdDlv();
		}

	}

	// 운송장 정보 입력 팝업
	function procInvcInfoPop(flag) {

		if (flag == "show") {
			$("#popInvc").addClass("on");
		}
		else {
			$("#popInvc").removeClass("on");
		}

	}

	// 상품배송
	function updateFreeShareProdDlv() {

		if (!confirm("상품을 배송 하시겠습니까?")) {
			return;
		}

		var dlvWayCd = $("#dlvWayCd").val();
		if (dlvWayCd == "2") {
			if ($("#invcNo").val() == null || $("#invcNo").val() == "") {
				alert("송장번호를 입력하셔야 합니다.");
				return;
			}
		}

		$("#procStatCd").val("22");
		$("#ordStatCd").val("40");

		var url = "/admin/order/updateFreeShareProdDlv.do";
		$.ajax({
		    type: 'post'
		    , async: true
		    , dataType:'json'
		    , url: url
		    , data: $("#form").serialize()
		    , success: function(data) {
		    	var rtnMsg = data.rtnMsg;
		    	alert(rtnMsg);

		    	$("#form").attr("action","/admin/order/selectFreeShareConfDtl.do").attr("target", "").submit();

		    }
		    , error: function(data, status, err) {
		    	alert('서버와의 통신이 실패했습니다. 다시 시도해 주십시오.');
		    }
		});

	}

	// 배송완료
	function updateDlvComplete() {

		if (!confirm("배송완료 처리 하시겠습니까?")) {
			return;
		}

		$("#procStatCd").val("23");

		var url = "/admin/order/updateDlvComplete.do";
		$.ajax({
		    type: 'post'
		    , async: true
		    , dataType:'json'
		    , url: url
		    , data: $("#form").serialize()
		    , success: function(data) {
		    	var rtnMsg = data.rtnMsg;
		    	alert(rtnMsg);

		    	$("#form").attr("action","/admin/order/selectFreeShareConfDtl.do").attr("target", "").submit();

		    }
		    , error: function(data, status, err) {
		    	alert('서버와의 통신이 실패했습니다. 다시 시도해 주십시오.');
		    }
		});

	}

	// 목록
	function goList() {

		$("#form").attr("action","/admin/order/selectListFreeShare.do").attr("target", "").submit();

	}

	/* ======================================================================
	Function : 우편번호 검색
	Return   : 없음
	========================================================================= */
	function getAddrInfo(zip_code,jiben_addr,road_addr) {
		$("#zipCd").val(zip_code);
		$("#jusoDoro").val(road_addr);
		$("#jusoJiben").val(jiben_addr);
	}

	/* ======================================================================
	Function : 배송조회 팝업 - 제휴택배사
	Return   : 없음
	========================================================================= */
	function openDlvStatPopup(ORD_CD, PROD_CODE, PROD_SEQ, DLV_CMP_CD, INVC_NO){
		if(DLV_CMP_CD == "" || INVC_NO == ""){
			alert("배송정보를 확인해 주세요.");
			return;
		}
		var url = "/admin/order/openDlvStatPopup.do?ORD_CD="+ORD_CD+"&PROD_CODE="+PROD_CODE+"&PROD_SEQ="+PROD_SEQ;
		var width = "600";
		var height = "500";
		//var left = screen.width;
		//var top = (screen.height/2) - (height/2);
		var left = 200;
		var top = 200;
		window.open(url, 'dlvStatPopUp','width=' + width + ', height=' + height + ', screenY=' + top + ', screenX=' + left + ',toolbar=no, menubar=no, scrollbars=yes, resizeable=no');
	}

	/* ======================================================================
	Function : 배송조회 팝업 - 타 배송업체
	Return   : 없음
	========================================================================= */
	function openDlvURL(DLV_CMP_CD ,INVC_NO){
		if(DLV_CMP_CD == "" || INVC_NO == ""){
			alert("배송정보를 확인해 주세요.");
			return;
		}
		var param = new Object();
		param.DLV_CMP_CD = DLV_CMP_CD;
		param.INVC_NO = INVC_NO;
		$.ajax({
			type: 'post'
			, async: true
			, url: '/admin/order/selectDlvCompanyURL.do'
			, dataType:'json'
			, data:  param
			, success: function(data) {
					if (data.result == '' || data.result == null) {
						alert("다시 시도해 주세요.");
					} else {
						window.open(data.result+INVC_NO);
					}
			}
			, error: function(data, status, err) {
				alert('서버와의 통신이 실패했습니다.');
			}
		});
	}


	/* ======================================================================
	Function : 배송완료 시간 수정 처리 팝업
	Return   : 없음
	========================================================================= */
	function openModifyDlvTime(proc_stat_cd,ord_cd,prod_code,prod_seq,orgin_dlv_cmp_cd,invc_no,dlv_way_cd, ord_reg_date){
		var temp = ord_reg_date.replace("-","");
		temp = temp.replace("-","");
		temp = temp.replace(":","");
		temp = temp.replace(" ","");
		$("#temp_proc_stat_cd").val(proc_stat_cd);
		$("#temp_ord_cd").val(ord_cd);
		$("#temp_prod_code").val(prod_code);
		$("#temp_prod_seq").val(prod_seq);
		$("#temp_dlv_cmp_cd").val(orgin_dlv_cmp_cd);
		$("#invcNo").val(invc_no);
		$("#temp_dlv_way_cd").val(dlv_way_cd);

		var url = "/admin/order/dlvInfoModifyPopup.do";
		$("#orgin_dlv_cmp_cd").val(orgin_dlv_cmp_cd);
		$("#orgin_invc_no").val(invc_no);
		$("#reg_date").val(temp);
		$("#orgin_proc_stat_cd").val(proc_stat_cd);
		var width = "500";
		var height = "250";
		var left = 200;
		var top = 200;
		window.open('', 'dlvInfoModifyPopup','width=' + width + ', height=' + height + ', screenY=' + top + ', screenX=' + left + ',toolbar=no, menubar=no, scrollbars=yes, resizeable=no');
		$("#popupForm").attr("action", url).attr("target", "dlvInfoModifyPopup").submit();
	}

	/* ======================================================================
	Function : 배송완료 시간 수정 처리
	Return   : 없음
	========================================================================= */
	function modifyDlvTime(){
		var param = new Object();
		param.OLD_INVC_NO = $("#invcNo").val();
		param.INVC_NO = $("#temp_invc_no").val();
		param.PROC_STAT_CD = $("#temp_proc_stat_cd").val();
		param.ORD_CD = $("#temp_ord_cd").val();
		param.PROD_CODE = $("#temp_prod_code").val();
		param.PROD_SEQ = $("#temp_prod_seq").val();
		param.DLV_CMP_CD = $("#temp_dlv_cmp_cd").val();
		param.DLV_WAY_CD = $("#temp_dlv_way_cd").val();
		param.DLV_END_DT = $("#temp_dlv_end_dt").val();
		$.ajax({
			type: 'post'
			, async: true
			, url: '/admin/order/modifyDlvTime.do'
			, dataType:'json'
			, data:  param
			, success: function(data) {
					if (data.result == 0) {
						alert("다시 시도해 주세요.");
					} else {
						alert("배송정보 수정 하였습니다.");
						location.reload();
					}
			}
			, error: function(data, status, err) {
				alert('서버와의 통신이 실패했습니다.');
			}
		});
	}
</script>

<!-- content -->
<div id="content">
<form name="form" id="form" method="post">
	<input type="hidden" id="prodCode" name="prodCode" value="${result.selectListFreeShareConf.PROD_CODE }" />
	<input type="hidden" id="prodSeq" name="prodSeq" value="${result.selectListFreeShareConf.PROD_SEQ }" />
	<input type="hidden" id="ordCd" name="ordCd" value="${result.selectListFreeShareConf.ORD_CD }" />
	<input type="hidden" id="dlvWayCd" name="dlvWayCd" value="${result.selectListFreeShareConf.DLV_WAY_CD }" />
	<input type="hidden" id="DLV_CMP_CD" name="DLV_CMP_CD" value="${result.selectListFreeShareConf.DLV_CMP_CD }" />
	<input type="hidden" id="INVC_NO" name="INVC_NO" value="${result.selectListFreeShareConf.INVC_NO }" />
	<input type="hidden" id="procStatCd" name="procStatCd" value="" />
	<!-- top_content -->
	<div class="top_content">
		<!-- location -->
		<div class="location">
			<ul class="clear">
				<li class="first-child"><a href="#">Home</a></li>
				<li><a href="#">나눔 상품 정보</a></li>
				<li><a href="#" class="current">나눔 주문 상세</a></li>
			</ul>
		</div>
		<!-- //location -->
	</div>
	<!-- //top_content -->

	<!-- inner -->
	<div class="inner">
		<!-- tbl -->
		<div class="tbl mt10">
			<p class="tbl_tit"><img src="http://static.ssocio.net/admin/images/ico_admin.gif" alt="" />주문 정보</p>
			<table class="upd type">
				<colgroup>
					<col style="width:10%" />
					<col style="width:23%" />
					<col style="width:10%" />
					<col style="width:23%" />
					<col style="width:10%" />
					<col style="width:24%" />
				</colgroup>
				<tbody>
					<tr>
						<th scope="row">주문일시</th>
						<td>
							<fmt:parseDate var="ORD_REGIST_DT" value="${result.selectListFreeShareConf.ORD_REGIST_DT }" pattern="yyyy-MM-dd HH:mm:ss.S" />
							<fmt:formatDate var="ordRegistDt" value="${ORD_REGIST_DT }" pattern="yyyy-MM-dd HH:mm"/>
							${ordRegistDt }
						</td>
						<th scope="row" class="bl">주문번호</th>
						<td>
							${result.selectListFreeShareConf.ORD_CD }
						</td>
						<th scope="row" class="bl">판매채널</th>
						<td>
							${result.selectListFreeShareConf.CHAN }
						</td>
					</tr>
					<tr>
						<th scope="row">주문자명</th>
						<td>
							${result.selectListFreeShareConf.ORD_MBER_NM }
						</td>
						<th scope="row" class="bl">주문자ID</th>
						<td>
							${result.selectListFreeShareConf.ORD_MBER_EMAIL }
						</td>
						<th scope="row" class="bl">휴대폰번호</th>
						<td>
							${result.selectListFreeShareConf.ORD_MBER_PHONE }
						</td>
					</tr>
				</tbody>
			</table>
		</div>
		<!-- // tbl -->

		<!-- tbl -->
		<div class="tbl mt10">
			<p class="tbl_tit"><img src="http://static.ssocio.net/admin/images/ico_admin.gif" alt="" />나눔 신청자 정보</p>
			<table class="list">
				<colgroup>
					<col style="width:12%" />
					<col style="width:*" />
				</colgroup>
				<thead>
					<tr>
						<th scope="col">신청일시</th>
						<th scope="col">신청 메시지</th>
					</tr>
				</thead>
				<tbody>
					<tr>
						<td>
							<fmt:parseDate var="REQ_REGIST_DT" value="${result.selectListFreeShareConf.REQ_REGIST_DT }" pattern="yyyy-MM-dd HH:mm:ss.S" />
							<fmt:formatDate var="reqRegistDt" value="${REQ_REGIST_DT }" pattern="yyyy-MM-dd HH:mm"/>
							${reqRegistDt }
						</td>
						<td class="tl">
							${result.selectListFreeShareConf.REQ_MEMO }
						</td>
					</tr>
				</tbody>
			</table>
		</div>
		<!-- //tbl -->

		<!-- tbl -->
		<div class="tbl mt10">
			<p class="tbl_tit"><img src="http://static.ssocio.net/admin/images/ico_admin.gif" alt="" />주문 상품 정보</p>
			<table class="list">
				<colgroup>
					<col style="width:15%" />
					<col style="width:*" />
					<col style="width:16%" />
					<col style="width:6%" />
					<col style="width:8%" />
					<col style="width:8%" />
				</colgroup>
				<thead>
					<tr>
						<th scope="col" rowspan="2">판매자명(ID)</th>
						<th scope="col" rowspan="2">상품명</th>
						<th scope="col" rowspan="2">나눔신청기간</th>
						<th scope="col" rowspan="2">신청자수</th>
						<th scope="col" rowspan="2">나눔결정</th>
						<th scope="col" rowspan="2">상태</th>
						<th scope="col" colspan="4" class="bdb">배송정보</th>
					</tr>
					<tr>
						<th scope="col">택배사</th>
						<th scope="col">송장번호</th>
						<th scope="col">배송완료일</th>
						<th scope="col">수정</th>
					</tr>
				</thead>
				<tbody>
					<tr>
						<td>
							[${result.selectListFreeShareConf.SELL_MBER_GB_CD_NM }]<br />
							${result.selectListFreeShareConf.SELL_MBER_NM }<br />
							${result.selectListFreeShareConf.SELL_MBER_EMAIL }
						</td>
						<td>
							<div class="thumbnail_bx">
								<img src="${result.selectListFreeShareConf.PROD_IMG_URL }" alt="상품 이미지">
								<a href="javascript:goProdDtl(${result.selectListFreeShareConf.PROD_CODE });" class="prod_name alink">
									<span class="ellipsis" style="width:455px;">${result.selectListFreeShareConf.PROD_NM }</span>
								</a>
							</div>
						</td>
						<td>
							<fmt:parseDate var="FREESHARE_START_DT" value="${result.selectListFreeShareConf.FREESHARE_START_DT }" pattern="yyyy-MM-dd HH:mm:ss.S" />
							<fmt:formatDate var="freeShareStartDt" value="${FREESHARE_START_DT }" pattern="yyyy-MM-dd"/>
							<fmt:parseDate var="FREESHARE_END_DT" value="${result.selectListFreeShareConf.FREESHARE_END_DT }" pattern="yyyy-MM-dd HH:mm:ss.S" />
							<fmt:formatDate var="freeShareEndDt" value="${FREESHARE_END_DT }" pattern="yyyy-MM-dd"/>
							${freeShareStartDt } ~ ${freeShareEndDt }
						</td>
						<td>
							${result.selectListFreeShareConf.REQ_CNT }
						</td>
						<td>
							${result.selectListFreeShareConf.PROC_STAT_CD_NM }
						</td>
						<td>
							<c:choose>
								<c:when test="${result.selectListFreeShareConf.PROC_STAT_CD == '95' }">
									<c:choose>
										<c:when test="${result.selectListFreeShareConf.DLV_WAY_CD == '1' }">
											<a href="javascript:updateFreeShareDlvReq();" class="btn_w">배송신청</a>
										</c:when>
										<c:when test="${result.selectListFreeShareConf.DLV_WAY_CD == '2' }">
											<a href="javascript:procDlvProd();" class="btn_w">상품배송</a>
										</c:when>
										<c:otherwise>
											<a href="javascript:updateFreeShareProdDlv();" class="btn_w">상품배송</a>
										</c:otherwise>
									</c:choose>
								</c:when>
								<c:when test="${result.selectListFreeShareConf.PROC_STAT_CD == '22' }">
									<c:choose>
										<c:when test="${result.selectListFreeShareConf.DLV_WAY_CD == '1' }">
											<a href="javascript:openDlvStatPopup('${result.selectListFreeShareConf.ORD_CD}','${result.selectListFreeShareConf.PROD_CODE}','${result.selectListFreeShareConf.PROD_SEQ}','${result.selectListFreeShareConf.DLV_CMP_CD}','${result.selectListFreeShareConf.INVC_NO}');" class="btn_w" style="margin-bottom: 5px;">배송조회</a>
											<a href="javascript:updateDlvComplete();" class="btn_w">배송완료</a>
										</c:when>
										<c:when test="${result.selectListFreeShareConf.DLV_WAY_CD == '2' }">
											<a href="javascript:openDlvURL('${result.selectListFreeShareConf.DLV_CMP_CD}','${result.selectListFreeShareConf.INVC_NO}');" class="btn_w" target="_blank" style="margin-bottom: 5px;">배송조회</a>
											<a href="javascript:updateDlvComplete();" class="btn_w">배송완료</a>
										</c:when>
										<c:when test="${result.selectListFreeShareConf.DLV_WAY_CD == '3' or result.selectListFreeShareConf.DLV_WAY_CD == '4' }">
											<a href="javascript:updateDlvComplete();" class="btn_w">배송완료</a>
										</c:when>
										<c:otherwise>
											-
										</c:otherwise>
									</c:choose>
								</c:when>
								<c:otherwise>
									-
								</c:otherwise>
							</c:choose>
						</td>
						<td><custom:codeTag groupCode="DLV_CMP_CD" code="${result.selectListFreeShareConf.DLV_CMP_CD}"/></td>
						<td>${result.selectListFreeShareConf.INVC_NO}</td>
						<td>${result.selectListFreeShareConf.DLV_END_DT}</td>
						<td class="last">
							<c:if test="${result.selectListFreeShareConf.PROC_STAT_CD eq '22'
											or result.selectListFreeShareConf.PROC_STAT_CD eq '23'}">
								<c:if test="${not empty result.selectListFreeShareConf.DLV_CMP_CD
												and not empty result.selectListFreeShareConf.INVC_NO}">
									<a href="javascript:openModifyDlvTime('${result.selectListFreeShareConf.PROC_STAT_CD}','${result.selectListFreeShareConf.ORD_CD}','${result.selectListFreeShareConf.PROD_CODE}','${result.selectListFreeShareConf.PROD_SEQ}','${result.selectListFreeShareConf.DLV_CMP_CD}','${result.selectListFreeShareConf.INVC_NO}','${result.selectListFreeShareConf.DLV_WAY_CD}','${result.selectListFreeShareConf.ORD_REG_DATE}');" class="btn_w">수정</a>
								</c:if>
							</c:if>
						</td>
					</tr>
				</tbody>
			</table>
		</div>
		<!-- //tbl -->

		<!-- tbl -->
		<div class="tbl mt10">
			<p class="tbl_tit">
				<img src="http://static.ssocio.net/admin/images/ico_admin.gif" alt="" />배송지 정보[판매회원->구매회원]
				<c:choose>
					<c:when test="${result.selectListFreeShareConf.PROC_STAT_CD == '95' }">
						<a href="javascript:updateDlvInfo();" class="btn_r btn_abs">저장</a>
					</c:when>
				</c:choose>
			</p>
			<table class="upd">
				<colgroup>
					<col style="width:12%" />
					<col style="width:13%" />
					<col style="width:12%" />
					<col style="width:13%" />
					<col style="width:12%" />
					<col style="width:13%" />
					<col style="width:12%" />
					<col style="width:13%" />
				</colgroup>
				<tbody>
					<c:choose>
