<?xml version="1.0" encoding="UTF-8"?>
<!-- #################################################################### -->
<!-- Application Config -->
<!-- #################################################################### -->

<configuration>

    <!-- ==================================================================== -->
    <!-- System Connection -->
    <!-- ==================================================================== -->
    <!-- 접속 Domain (운영/개발/로컬 판단용) -->
    <domain>sharing.ssocio.co.kr</domain>
    <domainTest>192.168.0.1</domainTest>
    <domainLocal>127.0.0.1</domainLocal>

    <!-- IP Web (등록장비) -->
    <wepIp>192.168.0.1;127.0.0.1</wepIp>
    <wepIpTest>192.168.0.1;127.0.0.1</wepIpTest>
    <wepIpLocal>192.168.0.107;127.0.0.1</wepIpLocal>

    <!-- IP Was (등록장비) -->
    <wasIp>192.168.0.1;127.0.0.1</wasIp>
    <wasIpTest>127.0.0.1</wasIpTest>
    <wasIpDev>127.0.0.1</wasIpDev>
    <wasIpLocal>192.168.0.107;192.168.0.107</wasIpLocal>

    <!-- Port Was (등록장비) -->
    <wasPort>8011;8012;8021;8031;8032;8033</wasPort>
    <wasPortDev>8011;8012;8021;8031;8032;8033</wasPortDev>
    <wasPortLocal>8080</wasPortLocal>
    <wasPortTest>8080</wasPortTest>
  
    <!-- WasApiPort (Jeus설정에 따라 포트는 동일할 수도 있음) -->
    <wasMallPcPort>28011</wasMallPcPort>
    <wasMallMobilePort>28012</wasMallMobilePort>
    <wasSellerPort>28021</wasSellerPort>
    <wasAdminPort>8080</wasAdminPort>
    <wasBatchPort>28023</wasBatchPort>
    
    
    
    

    
    
    <!-- com.ssocio.sharing.cmmn.elasticsearch 설정 -->
    
<!--        elasticIp  개발용 바꾸시고 커밋은 하지말아주세요. -->
<!--    <elasticIp>220.95.212.112</elasticIp> -->
    <elasticIp>127.0.0.1</elasticIp>
    
    <elasticPort>9300</elasticPort>
    <clusterName>ssocio_search</clusterName>
    <indexName>prods</indexName>
    <typeName>prod</typeName>
    
        <indexNameTag>favo_tag</indexNameTag>
        <typeNameTag>tag</typeNameTag>

    <!-- ==================================================================== -->
    <!-- Context -->
    <!-- ==================================================================== -->
    <context></context>

    <!-- ==================================================================== -->
    <!-- Image Root Path -->
    <!-- ==================================================================== -->
    <uploadRootPath>/data/ssocio</uploadRootPath>
    <uploadRootPathDev>/data/ssocio/</uploadRootPathDev>
    <uploadRootPathLocal>C:/</uploadRootPathLocal>

    <!-- ==================================================================== -->
    <!-- MISC Root Path -->
    <!-- ==================================================================== -->
    <uploadMiscRootPath>/data/ssocio</uploadMiscRootPath>
    <uploadMiscRootPathDev>/data/ssocio/</uploadMiscRootPathDev>
    <uploadMiscRootPathLocal>D:/</uploadMiscRootPathLocal>
    
      <!-- uri -->
      <uriCmmnCodeSync>/admin/instance/cmmnCodeSyncReload.do</uriCmmnCodeSync> <!-- 공통코드동기화 -->


    <!-- ==================================================================== -->
    <!-- Image Url Path -->
    <!-- ==================================================================== -->
    <!-- <uploadUrlPath>http://stg.cdn.ssocio.com/</uploadUrlPath>
    <uploadUrlPathDev>http://stg.cdn.ssocio.com/</uploadUrlPathDev>
    <uploadUrlPathLocal>http://stg.cdn.ssocio.com/</uploadUrlPathLocal> -->
    <uploadUrlPath>http://cdn.ssocio.com/</uploadUrlPath>
    <uploadUrlPathDev>http://cdn.ssocio.com/</uploadUrlPathDev>
    <uploadUrlPathLocal>http://cdn.ssocio.com/</uploadUrlPathLocal>
    <!-- ==================================================================== -->
    <!-- Batch Ip (Batch가실행되는Was의사설아이피, Batch구동지정여부 판단) -->
    <!-- ==================================================================== -->
    <batchIp>192.168.0.1</batchIp>
    <batchIpTest>192.168.0.1</batchIpTest>
    <batchIpLocal>127.0.0.1</batchIpLocal>

    <!-- ==================================================================== -->
    <!-- Batch Call Url -->
    <!-- Was에서 자신의 bat가 실행되어야 하므로 포트는 8380 -->
    <!-- 개발기인 경우 구동하는 인스턴스의 port -->
    <!-- ==================================================================== -->
    <!-- <batchCallUrl>http://localhost:8380/bat/bat.batchCall</batchCallUrl> -->
    <!-- <batchPrpareUrl>http://localhost:8380/bat/bat.batchPrpare</batchPrpareUrl> -->

</configuration>
