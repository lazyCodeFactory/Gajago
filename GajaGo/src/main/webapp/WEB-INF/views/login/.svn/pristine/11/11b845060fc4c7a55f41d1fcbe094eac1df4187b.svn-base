	/**
	 * <pre>
	 * 나눔  수락
	 * @param inParam
	 * @return
	 * @throws Exception
	 * @date 2015. 10. 13. 오전 2:27:12
	 * @author Kang
	 * @see com.ssocio.sharing.service.seller
	 * </pre>
	 */
	@Transactional
	@Override
	public void updateFreeShareConfirm(ModelAndView mav, Map<String,Object> inParam) throws Exception {
		
		LoggerUtil.writeMap(inParam);
		
		// 수락할 회원일련번호
		String mberSeq = (String)inParam.get("mberSeq");
		inParam.remove("mberSeq");
		
		//신청 완료 되었는지 확인(주문서 확인)
		inParam.put("procStatCd", "95");
		int freeCntRtn = sellerOrderDao.selectFreeShareOrdCount(inParam);
		
		
		if(freeCntRtn > 0){
			
			mav.addObject("rtnMsg", "해당 상품은 이미 수락이 완료 되었습니다.");
			mav.addObject("rtnCode", "NO");
			
		}else{
			// 나눔신청한 회원 나눔거절
			inParam.put("procStatCd", "96");
			int rejRtn = sellerOrderDao.updateFreeShareReq(inParam);
			
			if (rejRtn > 0) {
				// 선택한 회원 나눔수락
				inParam.put("procStatCd", "95");
				inParam.put("mberSeq", mberSeq);
				int confRtn = sellerOrderDao.updateFreeShareReq(inParam);
				
				if (confRtn > 0) {
					
					// 상품상태 나눔완료
					inParam.put("prodStatCd", CmmnCnst.cmmn1);				// 나눔완료
					sellerOrderDao.updateProdStatCd(inParam);
					
					// 신청정보 조회
					Map<String, Object> ordMap = new HashMap<String, Object>();
					ordMap = sellerOrderDao.selectProdSubmitInfo(inParam);
					String selectOrdCd = sellerOrderDao.selectOrdInfoSeq(inParam);
					// 주문정보 생성
					ordMap.put("ordCd",selectOrdCd);
					
					// 주문정보 생성
					String ordCd = sellerOrderDao.insertOrdInfo(ordMap);
					
					if (!StringUtil.isEmpty(ordCd)) {
						ordMap.put("ORD_CD", ordCd);
						
						LoggerUtil.writeMap(ordMap);
						
						// 주문 배송지정보 생성
						int dlvCnt = sellerOrderDao.insertOrdDlvInfo(ordMap);
						
						if (dlvCnt > 0) {
							
							// 주문상품정보 생성
							ordMap.put("PROC_STAT_CD", inParam.get("procStatCd"));
							ordMap.put("PROD_SEQ", CmmnCnst.cmmn1);
							ordMap.put("DLV_GRP_CODE", ordCd+"0001");
							int ordProdCnt = sellerOrderDao.insertOrdProdInfo(ordMap);
							
							if (ordProdCnt > 0) {
								mav.addObject("rtnMsg", CmmnCnst.FREE_REQ_OK);
								mav.addObject("rtnCode", "OK");
							}
							else {
								mav.addObject("rtnMsg", CmmnCnst.PROC_MSG_N);
								mav.addObject("rtnCode", "N");
							}
						}
						else {
							mav.addObject("rtnMsg", CmmnCnst.PROC_MSG_N);
							mav.addObject("rtnCode", "N");
						}
					}
					else {
						mav.addObject("rtnMsg", CmmnCnst.PROC_MSG_N);
						mav.addObject("rtnCode", "N");
					}
				}
				else {
					mav.addObject("rtnMsg", CmmnCnst.PROC_MSG_N);
					mav.addObject("rtnCode", "N");
				}
			}
			else {
				mav.addObject("rtnMsg", CmmnCnst.PROC_MSG_N);
				mav.addObject("rtnCode", "N");
			}
		}
	
		
	}
	
