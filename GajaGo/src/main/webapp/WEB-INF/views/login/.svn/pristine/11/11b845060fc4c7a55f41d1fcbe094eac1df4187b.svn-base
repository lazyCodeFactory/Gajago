	 * @비고
	 * 
	 * </pre>
	 */
	@Override
	public int updateOrderDtl(Map<String, Object> inParam) throws Exception {
		logger.debug("SellerOrderServiceImpl.updateOrderDtl [Start]");
		int outParam = 0;
		try {
			List<String> prodCodeList = (List<String>) inParam.get("L_PROD_CODE");
			List<String> procStatCdList = (List<String>) inParam.get("L_PROC_STAT_CD");
			List<String> ordStatCdList = (List<String>) inParam.get("L_ORD_STAT_CD");
			List<String> prodSeqList = (List<String>) inParam.get("L_PROD_SEQ");
			for (int i = 0; i < prodCodeList.size(); i++) {
				inParam.put("P_PROD_CODE", prodCodeList.get(i));
				inParam.put("P_PROC_STAT_CD", procStatCdList.get(i));
				inParam.put("P_ORD_STAT_CD", ordStatCdList.get(i));
				inParam.put("P_PROD_SEQ", prodSeqList.get(i));
				outParam = sellerOrderDao.updateOrderProdStat(inParam);
				outParam = sellerOrderDao.updateOrderProcStat(inParam);
				sellerOrderDao.insertOrderShipping(inParam);
				sellerOrderDao.insertOrdDlv(inParam);
			}
		} catch (Exception e) {
			e.printStackTrace();
		}
		return outParam;
	}
	
