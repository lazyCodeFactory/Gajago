 					<tr> 
 						<th scope="row">총 정산금액</th>
						<td class="tr"> <fmt:formatNumber value="${cpltShareAdj.selectCpltShareAmtCnt.F_ADJ_AMT}" type="number" /></td>
						<th scope="row">총 정산건수</th>
						<td class="tr"><fmt:formatNumber value="${cpltShareAdj.selectCpltShareAmtCnt.F_ADJ_CNT}" type="number" />  </td>
 					</tr> 
 					</c:if>
 					<c:if test="${cpltShareAdj.selectCpltShareAmtCnt eq null}">
 					<tr> 
 						<th scope="row">총 정산금액</th>
						<td class="tr"> 0 </td>
						<th scope="row">총 정산건수</th>
						<td class="tr"> 0 </td>
 					</tr> 
 					</c:if>
 				</tbody>
 			</table>
 		</div> 
		<!-- //tbl -->

		<div class="tbl mt10">
               <table class="list">
                   <colgroup>
                       <col style="width:8%" />
                        <col style="width:7%" />
                        <col style="width:8%" />
						<col style="width:8%" />
                        <col style="width:9%" />
                        <col style="width:8%" />
                        <col style="width:9%" />
                        <col style="width:9%" />
                        <col style="width:9%" />
                        <col style="width:9%" />
                        <col style="width:6%" />
                        <col style="width:8%" />
                   </colgroup>
                   <thead>	
                       <tr>
                            <th scope="col" colspan="4" class="bdb">판매금액(A)</th>
                            <th scope="col" colspan="6" class="bdb">공제금액(B)</th>
