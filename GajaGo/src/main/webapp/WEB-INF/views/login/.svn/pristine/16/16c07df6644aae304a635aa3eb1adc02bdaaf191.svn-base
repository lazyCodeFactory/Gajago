				$('#ACNT_NM').attr('disabled',true);
				$('#bank').attr('disabled',true);
				$('#BANK_ACNT_NO').attr('disabled',true);

				$('#accountChek').attr('onclick','return false');
				$('#accountChek').text('인증완료');
	    	  }else{
				  alert(data.resultMsg);
	    	  }


	      }
	      , complete: function() {
		        loadingForm.hideDialog();
		      }

