 /*
  * *********************************************************************
  * email 확인
  * *********************************************************************
  */
 function emailValidate(){
 	var regEmail = /([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;	//이메일 유효성
 	var regPw = /^[a-zA-Z0-9]{8,20}$/;	//비밀번호 유효성

 	var EMAIL = $("#MBER_EMAIL").val();

 	if(EMAIL.length == 0 ){
 		alert("이메일을 입력해주세요.");
 		$('#MBER_EMAIL').focus();
 		return;
 	}

 	if(!regEmail.test(EMAIL)) {
 	       alert('이메일 상세주소를 입력해주세요.');
 	       $("#MBER_EMAIL").focus();
 	       return false;
 	   }

 	return true;
 }


