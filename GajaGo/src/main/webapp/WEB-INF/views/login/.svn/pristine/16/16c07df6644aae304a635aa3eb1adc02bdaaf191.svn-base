function validate(){

	var ACNT_NM = $("#ACNT_NM").val();
	var BANK_ACNT_NO = $("#BANK_ACNT_NO").val();
	var bank = $("#bank option:selected").val();

	if(ACNT_NM.length == 0 ){
		alert("예금주명을 입력해주세요.");
		$('#ACNT_NM').focus();
		return;
	}

	if(BANK_ACNT_NO.length == 0 ){
		alert("계좌번호를 입력해주세요.");
		$('#ACNT_NM').focus();
		return;
	}

	if(bank == "N" ){
		alert("은행을 선택해주세요.");
		return;
	}

	return true;
}

