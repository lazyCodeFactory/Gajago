/*
 *	콤마처리
 */
function commify(n) {
 	var reg = /(^[+-]?\d+)(\d{3})/;   // 정규식
 	n += '';                          // 숫자를 문자열로 변환

 	while (reg.test(n))
   	n = n.replace(reg, '$1' + ',' + '$2');

 	return n;
}

function goDtl(code){
	var form = document.prodFrm;
	form.PROD_CODE.value = code;
	form.action = "/mobile/shar/selectSharDtl.do?PROD_CODE="+code;
	form.submit();
}
