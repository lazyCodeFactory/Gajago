 *Date|author| 변경내용
 *2015. 6. 12.|S.B.Lee | 신규
 * @Todo
 * </pre>
 */
public class SellerSession implements Serializable {
	private static final long serialVersionUID = 1L;
	
	private String MBER_EMAIL = "";			// 아이디
	private String MBER_PASS = "";			//패스워드 아이디
	private String MBER_SEQ = "";
	private String MBER_NM = "";
	private String REGIST_DT = "";
	private String MBER_GB_CD = "";
	private String MBER_REG_GB_CD = "";
	private String MBER_STAT_CD = "";
	private String CMPNY_NM = "";
	private String retCode = "";
	private int FAIL_CNT = 0;				//로그인실패횟수
	private boolean isLogin = false;//로그인 여부
	
	
	public String getCMPNY_NM() {
		return CMPNY_NM;
	}
	public void setCMPNY_NM(String cMPNY_NM) {
		CMPNY_NM = cMPNY_NM;
	}
	public String getMBER_STAT_CD() {
		return MBER_STAT_CD;
	}
	public void setMBER_STAT_CD(String mBER_STAT_CD) {
		MBER_STAT_CD = mBER_STAT_CD;
	}
	public String getMBER_GB_CD() {
		return MBER_GB_CD;
	}
	public void setMBER_GB_CD(String mBER_GB_CD) {
		MBER_GB_CD = mBER_GB_CD;
	}
	public String getRetCode() {
		return retCode;
	}
	public void setRetCode(String retCode) {
		this.retCode = retCode;
	}
	public String getMBER_NM() {
		return MBER_NM;
	}
	public void setMBER_NM(String mBER_NM) {
		MBER_NM = mBER_NM;
	}

	public String getREGIST_DT() {
		return REGIST_DT;
	}
	public void setREGIST_DT(String rEGIST_DT) {
		REGIST_DT = rEGIST_DT;
	}
	public String getMBER_EMAIL() {
		return MBER_EMAIL;
	}
	public void setMBER_EMAIL(String mBER_EMAIL) {
		MBER_EMAIL = mBER_EMAIL;
	}
	public String getMBER_PASS() {
		return MBER_PASS;
	}
	public void setMBER_PASS(String mBER_PASS) {
		MBER_PASS = mBER_PASS;
	}
	public String getMBER_SEQ() {
		return MBER_SEQ;
	}
	public void setMBER_SEQ(String mBER_SEQ) {
		MBER_SEQ = mBER_SEQ;
	}
	public int getFAIL_CNT() {
		return FAIL_CNT;
	}
	public void setFAIL_CNT(int fAIL_CNT) {
		FAIL_CNT = fAIL_CNT;
	}
	public boolean isLogin() {
		return isLogin;
	}
	public void setLogin(boolean isLogin) {
		this.isLogin = isLogin;
	}
	public void setMBER_REG_GB_CD(String MBER_REG_GB_CD) {
		this.MBER_REG_GB_CD = MBER_REG_GB_CD;
	}
	public String getMBER_REG_GB_CD() {
		return MBER_REG_GB_CD;
	}
	
	
	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("SellerSession [MBER_EMAIL=");
		builder.append(MBER_EMAIL);
		builder.append(", MBER_PASS=");
		builder.append(MBER_PASS);
		builder.append(", MBER_SEQ=");
		builder.append(MBER_SEQ);
		builder.append(", MBER_NM=");
		builder.append(MBER_NM);
		builder.append(", REGIST_DT=");
		builder.append(REGIST_DT);
		builder.append(", MBER_GB_CD=");
		builder.append(MBER_GB_CD);
		builder.append(", MBER_REG_GB_CD=");
		builder.append(MBER_REG_GB_CD);
		builder.append(", MBER_STAT_CD=");
		builder.append(MBER_STAT_CD);
		builder.append(", CMPNY_NM=");
		builder.append(CMPNY_NM);
		builder.append(", retCode=");
		builder.append(retCode);
		builder.append(", FAIL_CNT=");
		builder.append(FAIL_CNT);
		builder.append(", isLogin=");
		builder.append(isLogin);
		builder.append("]");
		return builder.toString();
	}
	

	
}
