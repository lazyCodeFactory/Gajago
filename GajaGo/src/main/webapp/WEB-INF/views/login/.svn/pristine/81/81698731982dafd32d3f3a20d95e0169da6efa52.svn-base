<script type="text/javascript">

$(document).ready(function() {
	// 헤더 set
	header.type	= 2; // 0 : 타이틀영역만존재, 1 : 검색HEADER, 2 : SELECT TYPE HEADER, 3 : AD MENU TYPE HEADER, 4 : SUB TYPE HEADER
	header.title = "셰어링 주문내역 상세";
	document.title = "셰어링 주문내역 상세";
	header.method = "POST";
	header.backUrl = "/mobile/buy/selectListSharOrderHist.do";
	header.form = $("#frm");
	header.callback = f_headerCallBack();
	initHeader();
	
	displayFooter(false);
	
	// 주소 확인 닫기 이벤트
	$("#btn_close").click(function() {
		popup_hide();
	});
	
});

/* ======================================================================
Function : 헤더 콜백 처리
Return   : 없음
========================================================================= */
function f_headerCallBack() {
	$("#ACTION").val("list");
}

/* ======================================================================
Function : 공통 화면 호출
Return   : 없음
========================================================================= */
function f_goCommCall(url, sOrdCd, sProdCd, sProdSeq, sScrGubun) {
	var arrProdCd = new Array();
	var arrProdSeq = new Array();
	
	if(sScrGubun != "prodPost" && sScrGubun != "shipView" && sScrGubun != "retShipView" && sScrGubun != "colShipView") {
		if(sScrGubun == "orderCancel") {
			sConfMsg = "선택하신 상품을 주문 취소 하시겠습니까?";
		}else if(sScrGubun == "excReq") {
			sConfMsg = "선택하신 상품을 교환 신청 하시겠습니까?";
		}else if(sScrGubun == "takeBackReq") {
			sConfMsg = "선택하신 상품을 반품 신청 하시겠습니까?";
		}
		
		if(confirm(sConfMsg) == false) { return; }
	}
	
	arrProdCd.push(sProdCd);
	arrProdSeq.push(sProdSeq);
	$("#S_ORD_CD").val(sOrdCd);
	$("#S_PROD_CD").val(arrProdCd);
	$("#S_PROD_SEQ").val(arrProdSeq);
	$("#S_SCR_GUBUN").val(sScrGubun);
	$("#frm").attr("action", url).submit();
}

/* ======================================================================
Function : 구매확정
Return   : 없음
========================================================================= */
function f_saveBuyFix(sOrdCd, sProdCd, sProdSeq) {
	
	if(confirm("선택하신 상품을 구매확정 하시겠습니까?") == false) { return; }
	
	$.ajax({
	    type: "post",
	    async: true,
	    url: "/mobile/buy/saveBuyFix.do",
	    data: {"S_ORD_CD":sOrdCd, "S_PROD_CD":sOrdCd, "S_PROD_SEQ":sProdSeq, "I_PROC_STAT_CD":"86"},
	    success: function(data) {
	    	if(data.result > 0) {
				alert("구매확정이 완료 되었습니다.");
				f_searchBuySharList("list");
			}else {
				alert("구매확정에 실패했습니다.");
			}
	    },
	    error: function(data, status, err) {
	      alert('서버와의 통신이 실패했습니다.');
	    }
	});
}
function f_searchBuySharList(sAction) {
	var sSearchPeri = $("#S_SEARCH_PERI").val();
    var url = "/mobile/buy/selectListSharOrderHist.do";
    
    if(gf_isNull(sSearchPeri)) {
    	alert("조회기간은 필수 입력입니다.");
		return;
    }
    $("#frm").attr("method","get");
    $("#ACTION").val(sAction);
	$("#frm").attr("action", url).submit();
}
/* ======================================================================
Function : 셰어링 확정
Return   : 없음
========================================================================= */
function f_saveShareFix(sOrdCd, sProdCd, sProdSeq) {
	
	if(confirm("선택하신 상품을 셰어링 확정 처리를 하시겠습니까?") == false) { return; }
	
	$.ajax({
	    type: "post",
	    async: true,
	    url: "/mobile/buy/saveShareFix.do",
	    data: {"S_ORD_CD":sOrdCd, "S_PROD_CD":sProdCd, "S_PROD_SEQ":sProdSeq, "I_PROC_STAT_CD":"85"},
	    success: function(data) {
	    	if(data.result > 0) {
				alert("셰어링 확정이 완료 되었습니다.");
				f_searchBuySharList("list");
			}else {
				alert("셰어링 확정에 실패했습니다.");
			}
	    },
	    error: function(data, status, err) {
	      alert('서버와의 통신이 실패했습니다.');
	    }
	});
}

/* ======================================================================
Function : 주소 확인 레이어
Return   : 없음
========================================================================= */
function f_addrConfLay(sOrdCd, sProdCd, sProdSeq, sScrGubun) {
	var sConfMsg = "";	
	if(sScrGubun == "retReq") {
		sConfMsg = "선택하신 상품을 반납신청 하시겠습니까?";
	}else if(sScrGubun == "colReq") {
		sConfMsg = "선택하신 상품을 회수요청 하시겠습니까?";
	}
		
	if(confirm(sConfMsg) == false) { return; }
	
	var wd = $(window).width();
	var pop_wd = $('.public_stop').width();
	$('.public_stop').css({"left":((wd-pop_wd)/2)+"px"}).show();
	popup_show();
	$("#S_SCR_GUBUN").val(sScrGubun);
	
	$.ajax({
	    type: "post",
	    async: true,
	    url: "/mobile/buy/selectAddrConf.do",
	    data: {"S_ORD_CD":sOrdCd, "S_PROD_CD":sProdCd, "S_PROD_SEQ":sProdSeq},
	    success: function(data) {
	    	if(data.list.buyOrdAddr.length > 0) {
	    		var sRcvNm = data.list.buyOrdAddr[0].RCV_NM;
		    	var sDlvZipCd = data.list.buyOrdAddr[0].DLV_ZIP_CD;
		    	var sDlvAddr = data.list.buyOrdAddr[0].DLV_ADDR;
		    	var sDlvAddrDtl = data.list.buyOrdAddr[0].DLV_ADDR_DTL;
		    	var sContact = data.list.buyOrdAddr[0].CONTACT;
		    	var sSecuNoUseYn = data.list.buyOrdAddr[0].SECU_NO_USE_YN;
		    	var sForVisit = data.list.buyOrdAddr[0].FOR_VISIT.dateFormat(".");
		    	var sSecuNoUseNm = "";
		    	
		    	$("#S_ORD_CD").val(sOrdCd);
		    	$("#S_PROD_CD").val(sProdCd);
		    	$("#S_PROD_SEQ").val(sProdSeq);
		    	$("#I_RCV_NM").val(sRcvNm);
		    	$("#I_ZIP_CODE").val(sDlvZipCd);
		    	$("#I_BAS_ADDR").val(sDlvAddr);
		    	$("#I_DTL_ADDR").val(sDlvAddrDtl);
		    	$("#I_CONTACT").val(sContact);
		    	$("#I_SECU_NO_USE_YN").val(sSecuNoUseYn);
		    	
		    	if(sSecuNoUseYn == "Y") {
		    		sSecuNoUseNm = "안심번호 사용";
		    	}
		    	
		    	$("#rcvNm").html(sRcvNm);
		    	$("#dlvZipCd").html(sDlvZipCd);
		    	$("#dlvAddr").html(sDlvAddr);
		    	$("#dlvAddrDtl").html(sDlvAddrDtl);
		    	$("#contact").html(sContact);
		    	$("#secuNoUseYn").html(sSecuNoUseNm);
		    	$("#forVisit").html(sForVisit);
	    	}
	    },
	    error: function(data, status, err) {
	      alert('서버와의 통신이 실패했습니다.');
	    }
	});
}

/* ======================================================================
Function : 주소변경 화면 호출
Return   : 없음
========================================================================= */
function f_addrChgCall() {
	var url = "/mobile/buy/selectAddrChg.do";
	$("#frm").attr("action", url).submit();
}

/* ======================================================================
Function : 주소확인 신청하기
Return   : 없음
========================================================================= */
function f_addrReq() {
	var sScrGubun = $("#S_SCR_GUBUN").val();
	var sChgGbCd;
	var sChgProcTypeCd;
	var sProcStatCd;
	
	if(sScrGubun == "retReq") {
		sChgGbCd = "80";
		sChgProcTypeCd = "80";
		sProcStatCd = "87";
	}else if(sScrGubun == "colReq") {
		sChgGbCd = "90";
		sChgProcTypeCd = "110";
		sProcStatCd = "90";
	}else {
		sChgGbCd = "";
		sChgProcTypeCd = "";
		sProcStatCd = "";
	}
	
	$("#I_CHG_GB_CD").val(sChgGbCd);
	$("#I_CHG_PROC_TYPE_CD").val(sChgProcTypeCd);
	$("#I_PROC_STAT_CD").val(sProcStatCd);
	$("#I_RFND_CD").val("10");
	$("#I_CHG_REASON_CD").val("");
	$("#I_CHG_REASON").val("");
	$("#I_SHIP_MEMO").val("");
	
	$.ajax({
		type: "post",
		async: true,
		url: "/mobile/buy/saveAddrRetColReq.do",
		dataType:"json",
		data: $("#frm").serialize(),
		success: function(data) {
			if(data.result > 0) {
				alert("신청이 완료 되었습니다.");
				f_searchBuySharList("list");
			}else {
				alert("신청이 실패 되었습니다.");
			}
		},
		error: function(data, status, err) {
			alert("서버와의 통신이 실패했습니다.");
		}
	});
}

/* ======================================================================
Function : 상품후기 수정 페이지
Return   : 없음
========================================================================= */
function selectProdReviewForm(boardSeq, prodCode, ordCd){
	$('#BOARD_SEQ').val(boardSeq);
	$('#PROD_CODE').val(prodCode);
	$('#ORD_CD').val(ordCd);
	$("#frm").attr("action", "/mobile/board/saveProdPostMod.do").submit();
}

/* ======================================================================
Function : 상품상세로 이동
Return   : 없음
========================================================================= */
function goProdDtlPage(prodCode){
	location.href = '/mobile/prod/selectProd.do?PROD_CODE=' + prodCode;
}

</script>
<form name="frm" id="frm" method="get">
