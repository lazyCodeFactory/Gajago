/* ======================================================================
Function : 셰어링 확정
Return   : 없음
========================================================================= */
function f_saveShareFix(sOrdCd, sProdCd, sProdSeq) {

	if(confirm("선택하신 상품을 셰어링 확정 처리를 하시겠습니까?") == false) { return; }

	$.ajax({
	    type: "post",
	    async: true,
	    url: "/mobile/buy/saveShareFix.do",
	    data: {"S_ORD_CD":sOrdCd, "S_PROD_CD":sProdCd, "S_PROD_SEQ":sProdSeq, "I_PROC_STAT_CD":"85"},
	    success: function(data) {
	    	if(data.result > 0) {
				alert("셰어링 확정이 완료 되었습니다.");
				f_searchBuySharList("list");
			}else {
				alert("셰어링 확정에 실패했습니다.");
			}
	    },
	    error: function(data, status, err) {
	      alert('서버와의 통신이 실패했습니다.');
	    }
	});
}

/* ======================================================================
Function : left pad
Return   : 없음
========================================================================= */
function f_lpad(sStr, sPad, len) {
	var str = sStr;
    while (str.length < len) {
        str = sPad + str;
    }
    return str;
}

/* ======================================================================
Function : 주소 확인 레이어
Return   : 없음
========================================================================= */
function f_addrConfLay(sOrdCd, sProdCd, sProdSeq, sScrGubun) {
	var sConfMsg = "";
	if(sScrGubun == "retReq") {
		sConfMsg = "선택하신 상품을 반납신청 하시겠습니까?";
	}else if(sScrGubun == "colReq") {
		sConfMsg = "선택하신 상품을 회수요청 하시겠습니까?";
	}

	if(confirm(sConfMsg) == false) { return; }
	
	$("#S_ORD_CD").val(sOrdCd);
	$("#S_PROD_CD").val(sProdCd);
	$("#S_PROD_SEQ").val(sProdSeq);

	var wd = $(window).width();
	var pop_wd = $('.public_stop').width();
	$('.public_stop').css({"left":((wd-pop_wd)/2)+"px"}).show();
	popup_show();
	$("#S_SCR_GUBUN").val(sScrGubun);

	$.ajax({
	    type: "post",
	    async: true,
	    url: "/mobile/buy/selectAddrConf.do",
	    data: {"S_ORD_CD":sOrdCd, "S_PROD_CD":sProdCd, "S_PROD_SEQ":sProdSeq},
	    success: function(data) {
	    	if(data.list.buyOrdAddr.length > 0) {
	    		var sRcvNm = data.list.buyOrdAddr[0].RCV_NM;
		    	var sDlvZipCd = data.list.buyOrdAddr[0].DLV_ZIP_CD;
		    	var sDlvAddr = data.list.buyOrdAddr[0].DLV_ADDR;
		    	var sDlvAddrDtl = data.list.buyOrdAddr[0].DLV_ADDR_DTL;
		    	var sContact = data.list.buyOrdAddr[0].CONTACT;
		    	var sSecuNoUseYn = data.list.buyOrdAddr[0].SECU_NO_USE_YN;
		    	var sForVisit = data.list.buyOrdAddr[0].FOR_VISIT.dateFormat(".");
		    	var sSecuNoUseNm = "";

		    	
		    	$("#I_RCV_NM").val(sRcvNm);
		    	$("#I_ZIP_CODE").val(sDlvZipCd);
		    	$("#I_BAS_ADDR").val(sDlvAddr);
		    	$("#I_DTL_ADDR").val(sDlvAddrDtl);
		    	$("#I_CONTACT").val(sContact);
		    	$("#I_SECU_NO_USE_YN").val(sSecuNoUseYn);

		    	if(sSecuNoUseYn == "Y") {
		    		sSecuNoUseNm = "안심번호 사용";
		    	}

		    	$("#rcvNm").html(sRcvNm);
		    	$("#dlvZipCd").html(sDlvZipCd);
		    	$("#dlvAddr").html(sDlvAddr);
		    	$("#dlvAddrDtl").html(sDlvAddrDtl);
		    	$("#contact").html(sContact);
		    	$("#secuNoUseYn").html(sSecuNoUseNm);
		    	$("#forVisit").html(sForVisit);
	    	}
	    },
	    error: function(data, status, err) {
	      alert('서버와의 통신이 실패했습니다.');
	    }
	});
}

/* ======================================================================
Function : 주소변경 화면 호출
Return   : 없음
========================================================================= */
function f_addrChgCall() {
	var url = "/mobile/buy/selectAddrChg.do";
	$("#frm").attr("action", url).submit();
}

/* ======================================================================
Function : 주소확인 신청하기
Return   : 없음
========================================================================= */
function f_addrReq() {
	var sScrGubun = $("#S_SCR_GUBUN").val();
	var sChgGbCd;
	var sChgProcTypeCd;
	var sProcStatCd;

	if(sScrGubun == "retReq") {
		sChgGbCd = "80";
		sChgProcTypeCd = "80";
		sProcStatCd = "87";
	}else if(sScrGubun == "colReq") {
		sChgGbCd = "90";
		sChgProcTypeCd = "110";
		sProcStatCd = "90";
	}else {
		sChgGbCd = "";
		sChgProcTypeCd = "";
		sProcStatCd = "";
	}

	$("#I_CHG_GB_CD").val(sChgGbCd);
	$("#I_CHG_PROC_TYPE_CD").val(sChgProcTypeCd);
	$("#I_PROC_STAT_CD").val(sProcStatCd);

	$.ajax({
		type: "post",
		async: true,
		url: "/mobile/buy/saveAddrRetColReq.do",
		dataType:"json",
		data: $("#frm").serialize(),
		success: function(data) {
			if(data.result > 0) {
				if(data.result == 87){
					alert("이미 반납신청 접수가 완료된 상품입니다.");
				}else{
					alert("신청이 완료 되었습니다.");	
				}
				
				f_searchBuySharList("list");
			}else {
				alert("신청이 실패 되었습니다.");
			}
		},
		error: function(data, status, err) {
			alert("서버와의 통신이 실패했습니다.");
		}
	});
}

