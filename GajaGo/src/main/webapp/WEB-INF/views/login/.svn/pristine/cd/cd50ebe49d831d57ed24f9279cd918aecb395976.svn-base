	    }
	});
}


/* ======================================================================
Function : 판매자별 판매구분
Return   : 없음
========================================================================= */
 
function doViewSellerType(flg){
	var flg = flg-1; // 배열을 위해 1만큼 감소
	// 판매자별 노출하지 말아야할 판매구분
	var sellerArry		= [
	              		 	["3", "sellerSpan", "bizSellerSpan"],			// 판매자 - SSOCIO
	              		 	["4", "5", "bizSellerSpan"],		// 판매자 - 개인 판매회원
	              		 	["3", "4", "sellerSpan"]		// 판매자 - 사업자 판매회원
	              		  ];
	//일단 판매구분 항목들은 전부 보이게 한다.
	for(i=1; i<=5; i++){
		$('#prodTypeCd'+i).parent().show();
	}
	// 판매자별 검색 조건도 일단 전부 노출
	$('#sellerSpan').show();
	$('#bizSellerSpan').show();
	
	// 배열에서 노출하지 말아야할 판매구분 코드를 가져와 숨김처리한다.
	for(j=0; j<=sellerArry[flg].length; j++){ 
		var tagId	= sellerArry[flg][j]
		$('#prodTypeCd'+tagId).parent().hide();
		$('#'+tagId).hide();
	}
 
	//판매구분
	var prodTypeCd = $('input[name=prodTypeCd]:checked').val();
	// 셰어링 최소 신청 기간 보이기 안보이기 설정

	if( $(':radio[id="seller2"]:checked').val() == 2 && flg == 1 && prodTypeCd == 2){
		$('#sharing5').hide();
		$('#sharingMaxTerm').attr("readonly",true);
	}else{
		if(prodTypeCd == 2){
			$('#sharing5').show();
			$('#sharingMaxTerm').attr("readonly",false);
		}
	}
	//사업자일때 기존에 나눔이 체크되 있을시 변경
	if(flg == 2){
		if(prodTypeCd == 3){
			clickProdType(1);
			$('#prodTypeCd1').attr('checked', 'checked');
		}
	}
	
}

/* ======================================================================
Function : 셰어링 가격 계산
========================================================================= */
function calMaxTerm(){
	var sellerCd = '${result.prodInfo.SELLER_CD}';

	if( $(':radio[id="seller2"]:checked').val() == 2 || sellerCd == 2){
		if($("#deposit").val().replace(/,/gi, "")  != "" && $("#sharingPrice").val().replace(/,/gi, "")  != ""){
			var calMaxTerm = $("#deposit").val().replace(/,/gi, "") / $("#sharingPrice").val().replace(/,/gi, "");
			$("#sharingMaxTerm").val(Math.ceil(calMaxTerm));
		}
		//$("#sharingMaxTerm").attr("readonly", true);
	}
}
