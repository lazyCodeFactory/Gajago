function onlyNumDecimalInput(obj){
 	  
	  var pattern = /^(\d{1,2}([.]\d{0,1})?)?$/; // 현재 value값이 소수점 둘째짜리 숫자이면 더이상 입력 불가
	  if(!pattern.test($(obj).val())){
		  
			$(obj).val($(obj).val().slice(0,-1));
	  }     
	
	return false;
 
}

