/* ======================================================================
Function : 판매자아이디 조회시 셋팅
========================================================================= */
function setSeller(sellerId, mberSeq){
	var chkFlg =  $('input[name="seller"]:checked').val();
	if(chkFlg == '2'){
		$('#sellerId').val(sellerId)
	}else if(chkFlg == '3'){
		$('#bizSellerId').val(sellerId);
	}
	
	$('#mberSeq').val(mberSeq);
	$('#popupMberSeq').val(mberSeq);

// 등록시 기본 출하지.
    <c:if test="${empty result.prodInfo.PROD_CODE}"> 
        makeShipmentList();
    </c:if>
}
/* ======================================================================
Function : 출하지셋팅
========================================================================= */
function setShipment(grpSeqList, grpNmList){
	$('#shipmentList').empty();
	$('#shipmentList').append('<a href="javascript:popupShipment();" class="btn_b left">출하지 선택</a>');
	for(var i=0; i<grpSeqList.length; i++){
		var html = '<p class="s_place"><span>'+grpNmList[i]+'</span>';
		html += '<input type="hidden" name="grpProdSeq" value="'+grpSeqList[i]+'"/>';
		html += '<a href="javascript:;" class="ml5 shipmentDel"><img src="http://static.ssocio.net/admin/images/btn_b_close.gif"></a></p>';
		$('#shipmentList').append(html);
	}
}
/* ======================================================================
Function : 이미지셋팅
========================================================================= */
function setImg(idx){
	for(var i=1; i<8; i++){
		$('#imgSpan1'+i).hide();
		$('#imgSpan2'+i).hide(); 
		$('#imgViewSpan1'+i).hide();
		$('#imgViewSpan2'+i).hide();
	}
	$('#imgSpan1'+idx).show();
	$('#imgSpan2'+idx).show();
	$('#imgViewSpan1'+idx).show();
	$('#imgViewSpan2'+idx).show();
	
	if(idx == 1){
		$('#pcImgTitle').text('메인 이미지');
		$('#mobileImgTitle').text('메인 이미지');
	}else{
		$('#pcImgTitle').text('추가 이미지'+(idx-1));
		$('#mobileImgTitle').text('추가 이미지'+(idx-1));
	}
 
	$('#productImg>td').removeClass('active');
	$('#productImg>td:eq('+(idx-1)+')').addClass('active');
	
}
/* ======================================================================
Function : 판매구분 라디오버튼 클릭
========================================================================= */
function clickProdType(typeCd){
	var prodCode = '${result.prodInfo.PROD_CODE}';

	if(typeCd == '1'){
		for(var i=1; i<11; i++) $('#sellsharing'+i).show();
		for(var i=1; i<8; i++) $('#sharing'+i).hide();
		for(var i=1; i<7; i++) $('#nanum'+i).hide();
		for(var i=1; i<7; i++) $('#rental'+i).hide();
		for(var i=1; i<5; i++) $('#p'+i).show();
		//$('#sell1').show();$('#sell2').show();
		$('#sellsharing2').show();	// 부가상품 주문옵션 선택
		$('#sellsharing3').show(); 	// 부가상품 주문옵션 상세
		$('#sellsharing8').show();	// 추가구성 주문옵션 선택
		$('#sellsharing9').show(); 	// 추가구성 주문옵션 상세
		$('#qtycnt').show(); // 판매수량
		 // 배송택배사 초기화
		 for(var i = 2; i < 7; i++) {
			$('#dlvWayCd' + i).parent().show();
		}
		 checkDlvType(1);
		$('#p5').hide();
		$('#p6').hide();
		$('#p7').hide();
	}else if(typeCd == '2'){
		for(var i=1; i<11; i++) $('#sellsharing'+i).hide();
		for(var i=1; i<8; i++) $('#sharing'+i).show();
		for(var i=1; i<7; i++) $('#nanum'+i).hide();
		for(var i=1; i<7; i++) $('#rental'+i).hide();
		for(var i=1; i<5; i++) $('#p'+i).show();
